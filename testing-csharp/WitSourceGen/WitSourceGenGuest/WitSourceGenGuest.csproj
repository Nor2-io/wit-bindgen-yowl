<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<WitCompilerGeneratedFilesOutputPath>Generated</WitCompilerGeneratedFilesOutputPath>
		<WitBindgenPath>c:\github\wit-bindgen-csharp\target\debug</WitBindgenPath>
	</PropertyGroup>

	<ItemGroup>
		<CompilerVisibleProperty Include="WitCompilerGeneratedFilesOutputPath" />
		<CompilerVisibleProperty Include="WitBindgenPath" />

		<AdditionalFiles Include="floats.wit" />
	</ItemGroup>


	<ItemGroup>
		<ProjectReference Include="..\WitSourceGen\WitSourceGen.csproj"
		                  OutputItemType="Analyzer"
		                  ReferenceOutputAssembly="false" />
	</ItemGroup>
</Project>
